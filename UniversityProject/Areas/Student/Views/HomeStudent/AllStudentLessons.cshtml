@model IEnumerable<UniversityProject.Models.Lesson>

@{
    ViewBag.Title = "AllStudentLessons";
}

<h2>AllStudentLessons</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vahed)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Vahed)
        </td>
    </tr>
}

</table>
